#ifndef _COMPLEX_H
#define _COMPLEX_H

typedef struct FCOMPLEX { float	r, i; } fcomplex;
typedef struct DCOMPLEX { double r, i; } dcomplex;
fcomplex Cinit(fcomplex);
fcomplex Cadd(fcomplex, fcomplex);
fcomplex Cmul(fcomplex, fcomplex);

#endif
